{
  "name": "@socialgouv/cdtn-monolog",
  "description": "CDTN log management : index logs and generate reports.",
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "sideEffects": false,
  "version": "1.5.2",
  "homepage": "https://github.com/SocialGouv/cdtn-monolog",
  "repository": {
    "type": "git",
    "url": "https://github.com/SocialGouv/cdtn-monolog.git"
  },
  "files": [
    "lib"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "dev": "jest --watch",
    "lint": "eslint src/ --ext .ts --fix",
    "test": "jest",
    "test-one": "jest analysis --watch",
    "monolog": "node -r ts-node/register --max-old-space-size=16000 src/index.ts",
    "build:bin": "yarn run ncc build src/bin/index.ts -o bin",
    "build:lib": "yarn tsc -p src/tsconfig.lib.json",
    "start": "node ./dist/index.js",
    "check:types": "tsc"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.13.0",
    "@socialgouv/cdtn-sources": "^4.45.0",
    "data-forge": "^1.8.17",
    "data-forge-fs": "^0.0.9",
    "date-fns": "^2.21.3",
    "esm": "^3.2.25",
    "fp-ts": "^2.10.5",
    "murmurhash-js": "^1.0.0",
    "node-fetch": "^2.6.1",
    "p-queue": "^6.6.2",
    "winston": "^3.3.3",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@socialgouv/eslint-config-recommended": "^1.75.0",
    "@types/jest": "^26.0.24",
    "@types/murmurhash-js": "^1.0.3",
    "@types/node-fetch": "^2.5.11",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "@zeit/ncc": "^0.22.3",
    "babel-jest": "^26.6.3",
    "eslint": "^7.30.0",
    "esm": "^3.2.25",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "prettier": "^2.3.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.1.0",
    "typescript": "4.3.5"
  },
  "jest": {
    "testTimeout": 30000,
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.test.tsx?$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint src/ --ext .ts"
    }
  }
}
