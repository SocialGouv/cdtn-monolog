{
  "name": "@socialgouv/cdtn-monolog",
  "description": "CDTN log management : index logs and generate reports.",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "version": "2.0.0",
  "homepage": "https://github.com/SocialGouv/cdtn-monolog",
  "repository": {
    "type": "git",
    "url": "https://github.com/SocialGouv/cdtn-monolog.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "dev": "jest --watch",
    "lint": "eslint src/ --ext .ts --fix",
    "test": "jest",
    "test-one": "jest analysis --watch",
    "monolog": "node -r ts-node/register --max-old-space-size=8192 src/index.ts",
    "build": "yarn run ncc build src/index.js -o dist",
    "start": "node ./dist/index.js",
    "check:types": "tsc"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.9.1",
    "@socialgouv/cdtn-sources": "^4.31.1",
    "data-forge": "^1.8.11",
    "data-forge-fs": "^0.0.8",
    "date-fns": "^2.16.1",
    "esm": "^3.2.25",
    "fp-ts": "^2.9.0",
    "murmurhash-js": "^1.0.0",
    "node-fetch": "^2.6.1",
    "p-queue": "^6.6.2",
    "winston": "^3.3.3",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@socialgouv/eslint-config-recommended": "^1.38.0",
    "@types/jest": "^26.0.15",
    "@types/murmurhash-js": "^1.0.3",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "@zeit/ncc": "^0.22.3",
    "babel-jest": "^26.5.2",
    "eslint": "^7.10.0",
    "esm": "^3.2.25",
    "husky": "^4.3.0",
    "jest": "^26.5.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.test.tsx?$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint src/ --ext .ts"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
