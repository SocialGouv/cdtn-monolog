{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": false,
    "declaration": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "lib": [
      "es2019"
    ],
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "outDir": "lib",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true
  },
  "exclude": [
    "node_modules",
    "lib",
    "bin"
  ]
}