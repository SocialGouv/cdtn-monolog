{
  "compilerOptions": {
    "allowJs": false,
    "declaration": true,
    "downlevelIteration": true,
    "lib": ["es2019"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "lib",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true
  },
  "exclude": ["node_modules", "lib", "bin"]
}
